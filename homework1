class Student:
    def __init__(self, name, surname, gender):
        self.name = name
        self.surname = surname
        self.gender = gender
        self.finished_courses = []
        self.courses_in_progress = []
        self.grades = {}

class Mentor:
    def __init__(self, name, surname):
        self.name = name
        self.surname = surname
        self.courses_attached = []

class Lecturer(Mentor):
    pass

class Reviewer(Mentor):
    pass

lecturer1 = Lecturer('Иван', 'Иванов')
reviewer1 = Reviewer('Пётр', 'Петров')

print(f"Лектор: {lecturer1.name} {lecturer1.surname}, курсы: {lecturer1.courses_attached}")
print(f"Проверяющий: {reviewer1.name} {reviewer1.surname}, курсы: {reviewer1.courses_attached}")

lecturer1.courses_attached += ['Python']
reviewer1.courses_attached += ['Python']

print(f"Лектор ведёт: {lecturer1.courses_attached}")
print(f"Проверяющий проверяет: {reviewer1.courses_attached}")